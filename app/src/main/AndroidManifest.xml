<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaoxiao.testrxjava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ToolbarActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".TestActivity"></activity>
        <activity android:name=".eventbus.EventBusActivity"></activity>
        <activity android:name=".service.ServiceActivity"></activity>
        <activity android:name=".testkeyboard.TestKeyBoardActivity"></activity>
        <activity android:name=".ListViewInScrollActivity"/>

        <service android:name=".service.LocalService"></service>
        <service android:name=".service.ForegroundService"></service>
        <service android:name=".service.RemoteService" android:process=":remote"></service>
    </application>

</manifest>
