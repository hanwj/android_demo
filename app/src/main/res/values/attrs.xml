        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MyCircleView">
        <attr name="cvRadius" format="dimension"/>
        <attr name="cvColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="menuOpen" format="boolean"/>
    </declare-styleable>

    <!-- 连击window -->
